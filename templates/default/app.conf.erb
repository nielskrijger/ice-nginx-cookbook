# Nginx proxy for <%= @resource.name %>
#
server {

  listen <%= @resource.port %>;

  server_name <%= @resource.server_name.is_a?(Array) ? @resource.server_name.join(' ') : @resource.server_name %>;

  access_log <%= @nginx.access_log %>;
  error_log <%= @nginx.error_log %> warn;

  <% for location in nginx.locations %>
  location <%= location.path %> {
    <% locations.config.each_pair do |key, value| %>
        <%= key %> <%= value %>;
    <% end %>
  }
  <% end %>
}
